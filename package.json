{
  "name": "42cabi",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prestart": "npm run doc",
    "start": "npm-run-all --parallel start:**",
    "start:client": "npm run dev",
    "start:server": "nodemon ./server/src/index.ts",
    "doc": "swagger-cli bundle ./server/openapi.yaml --outfile ./server/src/swagger.yaml --type yaml"
  },
  "dependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swagger-cli": "^4.0.4",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^1.0.7",
    "react-router-dom": "^5.3.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
